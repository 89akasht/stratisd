use std::collections::BTreeMap;
use std::path::PathBuf;

use types::Sectors;

#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct BlockDevSave {
    pub path: PathBuf,
    pub sectors: Sectors,
}

#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct StratSave {
    pub name: String,
    pub id: String,
    pub block_devs: BTreeMap<String, BlockDevSave>,
}
